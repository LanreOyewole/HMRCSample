        <property expression="$ctx:ERROR_DETAIL" name="HMRCSampleFault_SQ_LOG_EDetl_______________________####" xmlns:ns="http://org.apache.synapse/xsd"/>
        <property expression="$ctx:ERROR_EXCEPTION" name="HMRCSampleFault_SQ_LOG_EXcpt_______________________####" xmlns:ns="http://org.apache.synapse/xsd"/>
    </log>
</sequence>
